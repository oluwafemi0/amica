{
  "expo": {
    "name": "Amica",
    "slug": "amica",
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HANDS.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/HANDS.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "amica.visor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/HANDS.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/HANDS.png"
    },
    "extra": {
      "eas": {
        "projectId": "625f0b1a-2495-410b-b090-b67a6c7dd178"
      }
    }
  }
}
